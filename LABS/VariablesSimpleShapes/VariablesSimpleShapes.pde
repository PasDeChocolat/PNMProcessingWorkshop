// Set the size of the window:
size(900, 400);
// Color the background white:
background(255);
// Set the drawing color to black:
fill(0);

// Draw a plus:
rect(150, 180, 100, 40);
rect(180, 150, 40, 100);

// Draw a square:
rect(400, 150, 100, 100);

// Draw a circle:
ellipse(720, 180, 100, 100);

