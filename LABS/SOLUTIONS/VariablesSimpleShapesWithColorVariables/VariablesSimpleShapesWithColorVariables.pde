// Set the size of the window:
size(900, 400);

// Color the background white:
background(255);

// Create color variables:
color red   = color(255, 0, 0);
color green = color(0, 255, 0);

// Draw a plus:
fill(red);
rect(150, 180, 100, 40);
rect(180, 150, 40, 100);

// Draw a square:
fill(green);
rect(400, 150, 100, 100);

// Draw a circle:
fill(red);
ellipse(720, 180, 100, 100);

